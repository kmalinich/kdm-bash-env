{
	"default_module": "powerline.segments.common",
	"dividers": {
		"left": {
			"hard": " ",
			"soft": ""
		},
		"right": {
			"hard": " ",
			"soft": " "
		}
	},
	"segment_data": {
		"branch": {
			"after": "",
			"args": {
				"status_colors": true
			},
			"before": " "
		},
		"powerline.segments.common.net.hostname": {
			"after": "",
			"args": {
				"exclude_domain": true,
				"only_if_ssh": false
			},
			"before": ""
		},
		"powerline.segments.common.vcs.stash": {
			"after": "",
			"before": "⌆ "
		},
		"powerline.segments.common.wthr.weather": {
			"after": "",
			"args": {
				"icons": {
					"blustery": "💨 ",
					"cloudy": "☁️ ",
					"day": "☀️ ",
					"foggy": "🌫️ ",
					"night": "🌙 ",
					"not_available": "❓ ",
					"rainy": "🌧️ ",
					"snowy": "❄️ ",
					"stormy": "⛈️ ",
					"sunny": "☀️ ",
					"unknown": "⚠️ ",
					"windy": "💨 "
				},
				"temp_coldest": -30,
				"temp_format": "{temp:.0f}°",
				"temp_hottest": 45,
				"unit": "C"
			},
			"before": ""
		},
		"powerline.segments.shell.cwd": {
			"after": "",
			"args": {
				"dir_limit_depth": 3,
				"ellipsis": "..",
				"use_path_separator": true
			},
			"before": ""
		},
		"user": {
			"args": {
				"hide_user": "kdm"
			}
		}
	},
	"segments": {
		"left": [
			{
				"display": true,
				"function": "powerline.segments.common.wthr.weather",
				"priority": 50
			},
			{
				"function": "powerline.segments.common.net.hostname",
				"priority": 20
			},
			{
				"function": "powerline.segments.common.env.user",
				"priority": 30
			},
			{
				"function": "powerline.segments.shell.cwd",
				"priority": 10
			},
			{
				"function": "powerline.segments.common.vcs.branch",
				"priority": 40
			}
		],
		"right": [
			{
				"function": "powerline.segments.common.time.date",
				"priority": 60
			},
			{
				"function": "powerline.segments.shell.last_pipe_status",
				"priority": 70
			}
		]
	},
	"spaces": 1
}
