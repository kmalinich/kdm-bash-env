export C_CYN_BRT="${C_RST}\e[38;2;0;255;255m"
export C_GRN_BRT="${C_RST}\e[38;2;0;255;0m"
export C_GRY_BRT="${C_RST}\e[38;2;192;192;192m"
export C_ORN_BRT="${C_RST}\e[38;2;255;128;0m"
export C_PNK_BRT="${C_RST}\e[38;2;255;0;224m"
export C_PRP_BRT="${C_RST}\e[38;2;123;70;232m"
export C_RED_BRT="${C_RST}\e[38;2;255;0;0m"
export C_WHT_BRT="${C_RST}\e[38;2;255;255;255m"
export C_YLW_BRT="${C_RST}\e[38;2;255;255;0m"


# Normal+Bold
export C_BLK_BLD="${C_BLK}${C_BLD}"
