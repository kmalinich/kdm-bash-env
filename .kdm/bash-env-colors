#### Color variables ==start ####

# Output styles
export C_RST="\e[0m"
export C_BLD="\e[1m"
export C_DRK="\e[2m"
export C_FLS="\e[5m"
export C_INV="\e[7m"
export C_ITA="\e[3m"
export C_UDL="\e[4m"

export C_ALT="\e[41;5m"


# Normal
export C_BLK="${C_RST}\e[38;2;48;48;48m"
export C_BLU="${C_RST}\e[38;2;51;152;219m"
export C_CYN="${C_RST}\e[38;2;0;200;200m"
export C_GRN="${C_RST}\e[38;2;47;223;100m"
export C_GRY="${C_RST}\e[38;2;144;144;144m"
