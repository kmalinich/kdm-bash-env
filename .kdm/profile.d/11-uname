# Machine uname variables
UNAME_KERNEL_NAME="$(   uname -s)"
UNAME_KERNEL_RELEASE="$(uname -r)"
UNAME_MACHINE="$(       uname -m)"

export UNAME_KERNEL_NAME UNAME_KERNEL_RELEASE UNAME_MACHINE

