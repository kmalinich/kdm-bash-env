# shellcheck disable=SC2034

# kdm bash-env bash_profile arrays

# Ignore bland commands
ARRAY_HISTIGNORE=(
..
[bf]g
c
cd
clear
cs
dc
dl
dt
exit
g-gca
g-gcp
g-gi
g-gs
g-gu
go2sleep
l
l.
l1
la
ll
ll.
lld
lll
logout
ls
stayup
)
# Generate HISTIGNORE from array
STR_HISTIGNORE="${ARRAY_HISTIGNORE[*]}"
HISTIGNORE="${STR_HISTIGNORE//\ /:}"


# Array of editor aliases
ARRAY_EDITOR_ALIASES=(v vi vim nvim)

# Array of editor binaries
ARRAY_EDITOR_NAMES=(nvim vim vi)


# Entries at the end       of this array will end up at the beginning of MANPATH
# Entries at the beginning of this array will end up at the end       of MANPATH
ARRAY_MANPATH=(
"${HOME}/perl5/man"
/usr/local/share/man
/usr/pgsql-9.5/share/man
/usr/share/man/overrides
/usr/share/man/en
/usr/share/man
/usr/X11R6/share/man
/opt/X11/share/man
)

# Entries at the end       of this array will end up at the beginning of MIBDIRS
# Entries at the beginning of this array will end up at the end       of MIBDIRS
ARRAY_MIBDIRS=(
"${BASH_ENV_DIR_SNMP}/mibs"
/usr/share/snmp/mibs
)

ARRAY_MKDIR=(
"${BASH_ENV_DIR_KDM}"
"${BASH_ENV_DIR_SSH_CONFIG}"
"${BASH_ENV_DIR_SSH_SESSIONS}"
)

# Entries at the end       of this array will end up at the beginning of NODE_PATH
# Entries at the beginning of this array will end up at the end       of NODE_PATH
ARRAY_NODE_PATH=(
/usr/lib/nodejs
/usr/lib/node_modules
/usr/local/lib/node_modules
)

ARRAY_PACKAGE_MANAGERS=(
apt
