[color]
	branch      = on
	diff        = on
	interactive = on
	status      = on
	ui          = true

[color "branch"]
	current = yellow reverse
	local   = yellow
	remote  = green

[color "diff"]
	meta = yellow
	frag = magenta
	old  = red
	new  = green

[color "status"]
	added     = green
	changed   = cyan
	untracked = red

